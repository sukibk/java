import java.util.Scanner;

public class LabProgram {
   public static void main(String[] args) {
      Scanner scn = new Scanner(System.in);
      
      int a = scn.nextInt();
      int b = scn.nextInt();
      int c = scn.nextInt();
      
      int min;
      
      min = a;
      
      if(min>b)
      min = b;
      
      if(min>c)
      min = c;
      
      System.out.println(min);
   }
}
